{	
  "ci": {	
    "collect": {	
      "startServerCommand": "yarn start",	
      "url": [	
        "http://localhost:3000"
      ]
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", {"minScore": "99"}],
        "categories:accessibility": ["warn", {"minScore": "100"}]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }	
}